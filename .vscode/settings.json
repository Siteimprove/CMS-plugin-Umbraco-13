{
	"editor.formatOnSave": true,
	"eslint.enable": true,
	"eslint.run": "onType",
	"eslint.lintTask.enable": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": "explicit"
	},
	"eslint.format.enable": true,
	"csharp.format.enable": true,
	"cSpell.words": [
		"authed",
		"Backoff",
		"Bokm√•l",
		"classname",
		"cmsplugin",
		"datetime",
		"Decorrelated",
		"fancylab",
		"fancylib",
		"Gurli",
		"gurlis",
		"Millis",
		"Nextgen",
		"nextval",
		"Oneplatform",
		"Pendo",
		"personparam",
		"Protobuf",
		"rownum",
		"Siteimprove",
		"smallbox",
		"sysdate",
		"Unpublish"
	],
	"cSpell.ignorePaths": ["launchSettings.json", "settings.json"],
	"dotnet.defaultSolution": "Siteimprove.Umbraco13.Plugin.sln",
	"editor.rulers": [180],
	"editor.editorconfig.enabled": true
}
